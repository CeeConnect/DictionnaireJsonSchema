{
  "$id": "https://schemas.ceeconnect.fr/bundles/operation/bar/BAR-TH-150v14-1",
  "$schema": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/meta-schema.json",
  "title": "BAR-TH-150",
  "type": "object",
  "properties": {
    "batiment": {
      "title": "Logement",
      "type": "object",
      "properties": {
        "type": {
          "$ref": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/mixins/operation/bar/batiment/type.json#/$defs/immeuble"
        },
        "anciennete": {
          "$ref": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/mixins/operation/bar/batiment/anciennete.json#/$defs/existant"
        }
      },
      "required": [
        "type",
        "anciennete"
      ]
    },
    "codeDepartement": {
      "$ref": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/mixins/code-departement.json"
    },
    "logements": {
      "$ref": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/mixins/operation/bar/logements.json"
    },
    "typeUsage": {
      "$ref": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/mixins/operation/bar/type-usage.json#/$defs/chauffages"
    },
    "puissanceNominale": {
      "$ref": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/mixins/operation/puissance-nominale.json"
    }
  },
  "allOf": [
    {
      "if": {
        "properties": {
          "puissanceNominale": {
            "type": "number",
            "maximum": 400
          }
        },
        "required": [
          "puissanceNominale"
        ]
      },
      "then": {
        "properties": {
          "etas": {
            "$ref": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/mixins/operation/etas.json",
            "minimum": 102
          }
        },
        "required": [
          "etas"
        ]
      }
    },
    {
      "if": {
        "properties": {
          "puissanceNominale": {
            "type": "number",
            "exclusiveMinimum": 400
          }
        },
        "required": [
          "puissanceNominale"
        ]
      },
      "then": {
        "properties": {
          "cop": {
            "title": "Coefficient de performance énergétique",
            "type": "number",
            "minimum": 1.3
          }
        },
        "required": [
          "cop"
        ]
      }
    }
  ],
  "required": [
    "batiment",
    "codeDepartement",
    "logements",
    "typeUsage",
    "puissanceNominale"
  ],
  "examples": [
    {
      "batiment": {
        "type": "IMMEUBLE",
        "anciennete": "EXISTANT"
      },
      "codeDepartement": "32",
      "logements": 20,
      "typeUsage": "CHAUFFAGE",
      "puissanceNominale": 300,
      "etas": 105
    },
    {
      "batiment": {
        "type": "IMMEUBLE",
        "anciennete": "EXISTANT"
      },
      "codeDepartement": "13",
      "logements": 20,
      "typeUsage": "CHAUFFAGE_ECS",
      "puissanceNominale": 450,
      "cop": 1.8
    },
    {
      "batiment": {
        "type": "IMMEUBLE",
        "anciennete": "EXISTANT"
      },
      "codeDepartement": "62",
      "logements": 20,
      "typeUsage": "CHAUFFAGE_ECS",
      "puissanceNominale": 420,
      "cop": 1.5
    }
  ]
}