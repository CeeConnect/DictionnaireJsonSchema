{
  "$id": "https://schemas.ceeconnect.fr/bundles/archive",
  "$schema": "https://raw.githubusercontent.com/CeeConnect/DictionnaireJsonSchema/main/meta-schema.json",
  "title": "Archive",
  "type": "object",
  "properties": {
    "nom": {
      "title": "Nom",
      "type": "string",
      "maxLength": 240
    },
    "url": {
      "title": "Lien",
      "type": "string",
      "format": "uri",
      "pattern": "^https?://"
    }
  },
  "required": [
    "nom",
    "url"
  ]
}
